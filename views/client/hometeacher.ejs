                <form class="" action="" method="post">


                    <label for="x" style="font-size:20px;">ป.ตรี</label>
                    <div class="tab">

                        <div id="eduGroup" class="form-inline">

                            <div class="form-group">
                                <label for="x">ชื่อสถานศึกษา</label>
                                <input type="text" class="form-control" id="b_uniName" placeholder="">
                            </div>
                            <div class="form-group">
                                <label for="x">วุฒิการศึกษา</label>
                                <input type="text" class="form-control" id="b_degree" placeholder="">
                            </div>
                            <div class="form-group">
                                <label for="x">สาขาวิชา</label>
                                <input type="text" class="form-control" id="b_branch" placeholder="">
                            </div>
                            <div class="form-group">
                                <label for="x">ปีที่สำเร็จ</label>
                                <input type="text" class="form-control" id="b_graduateYear" placeholder="">
