            <!-- myThesis -->
            <div class="content" ng-show='isVisible === "manageThesis"'>
                <div class="alert alert-warning">
                    <strong>แจ้งเตือน!</strong> นิสิตปริญญาตรีไม่สามารถแก้ไขข้อมูลวิทยานิพนธ์ได้.
                </div>
                <div class="content_head">ข้อมูลวิทยานิพนธ์ของคุณ<span><i class="fa fa-print"></i></span></div>

                <form id="thesisdata">
                    <div class="form-group">
                        <label for="x">ชื่อวิทยานิพนธ์(ไทย)</label>
                        <input type="text" class="form-control" id="thesisTH" placeholder="">
                    </div>
                    <div class="form-group">
                        <label for="x">ชื่อวิทยานิพนธ์(อังกฤษ)</label>
                        <input type="text" class="form-control" id="thesisEN" placeholder="">
                    </div>

                    <div class="form-group">
                        <label for="x">อาจารย์ที่ปรึกษา</label>
                        <input type="text" class="form-control" id="advisor" placeholder="">
                    </div>

                    <div class="form-group">
                        <label for="x">อาจารย์ที่ปรึกษาร่วม</label>
